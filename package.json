    "discord.js": "^13.1.0",
    "distube": "^3.0.0-beta.38",
    "dotenv": "^10.0.0",
    "ffmpeg-static": "^4.4.0",
    "libsodium-wrappers": "^0.7.9",
    "pm2": "^5.1.0",
    "pretty-ms": "^7.0.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/node": "^16.9.0",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "eslint": "^7.32.0",
    "prettier": "^2.3.2",
    "prettier-plugin-organize-imports": "^2.3.3",
    "ts-node": "^10.2.0",
