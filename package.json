{
  "name": "liscia-discordbot",
  "version": "2.0.0",
  "description": "Discord bot for all purposes written in Discord.js and TypeScript",
  "main": "index.js",
  "author": "PoProstuWitold",
  "license": "MIT",
  "scripts": {
    "preinstall": "npm install -g pm2",
    "build-ts": "tsc",
    "build": "npm run build-ts && npm run copy-static-assets",
    "dev": "ts-node-dev src/index.ts",
    "js": "tsc && node dist/src/index.js",
    "watch": "tsc -w",
    "prod": "npm run build && node dist/src/index.js",
    "lint": "eslint . --config=.eslintrc.json --fix",
    "tailwind:dev": "postcss src/dashboard/src/public/styles/tailwind.css -o src/dashboard/src/public/styles/style.css",
    "tailwind:prod": "NODE_ENV=production postcss src/dashboard/src/public/styles/tailwind.css -o src/dashboard/src/public/styles/style.css",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "@discordjs/opus": "^0.7.0",
    "@discordjs/voice": "^0.8.0",
    "@distube/soundcloud": "^1.1.0",
    "@distube/spotify": "^1.1.0",
    "@distube/yt-dlp": "^0.0.2",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.13",
    "@types/node": "^16.9.0",
    "@types/shelljs": "^0.8.9",
    "@types/tailwindcss": "^2.2.1",
    "autoprefixer": "^10.3.7",
    "compression": "^1.7.4",
    "cssnano": "^5.0.8",
    "discord.js": "^13.6.0",
    "distube": "^3.3.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "handlebars": "^4.7.7",
    "hbs": "^4.2.0",
    "libsodium-wrappers": "^0.7.9",
    "postcss": "^8.3.9",
    "postcss-cli": "^9.0.1",
    "pretty-ms": "^7.0.1",
    "shelljs": "^0.8.4",
    "tailwindcss": "^2.2.17",
    "tslib": "^2.3.1",
    "util": "^0.12.4",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/hbs": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "eslint": "^7.32.0",
    "prettier": "^2.3.2",
    "prettier-plugin-organize-imports": "^2.3.3",
    "ts-node": "^10.2.0",
    "ts-node-dev": "^1.1.8",
    "tslint": "^6.1.3",
    "typescript": "^4.4.2"
  },
  "engines": {
    "node": "16.x"
  }
}
